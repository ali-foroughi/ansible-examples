logger:
    file: {{ default_core_log_dir_5g }}/amf.log

tls:
    enabled: {{ tls.enabled }}
    server:
      cacert: {{ default_core_configs_dir_5g }}/tls/ca.crt
      key: {{ default_core_configs_dir_5g }}/tls/amf.key
      cert: {{ default_core_configs_dir_5g }}/tls/amf.crt
    client:
      cacert: {{ default_core_configs_dir_5g }}/tls/ca.crt
      key: {{ default_core_configs_dir_5g }}/tls/amf.key
      cert: {{ default_core_configs_dir_5g }}/tls/amf.crt


amf:
    sbi:
      - addr: {{ amf.sbi }}
        port: 7777
    ngap:
      - addr: {{ amf.ngap }}
    guami:
      - plmn_id:
          mcc: {{ amf.mcc }}
          mnc: {{ amf.mnc }}
        amf_id:
          region: {{ amf.region }}
          set: {{ amf.set }}
    tai:
      - plmn_id:
          mcc: {{ amf.mcc }}
          mnc: {{ amf.mnc }}
        tac: {{ amf.tac }}
    plmn_support:
      - plmn_id:
          mcc: {{ amf.mcc }}
          mnc: {{ amf.mnc }}
        s_nssai:
          - sst: {{ amf.sst }}
    security:
        integrity_order : [ NIA2, NIA1, NIA0 ]
        ciphering_order : [ NEA0, NEA1, NEA2 ]
    network_name:
        full: neon
    amf_name: {{ amf.name }}

