#          - {{ nrf.sbi }}
#        port: 7777

scp:
    sbi:
      - addr: {{ scp.sbi }}
        port: 7777

parameter:
max:
  ue: {{ max.ue }}
  peer: {{ max.peer }}
pool:
time:
