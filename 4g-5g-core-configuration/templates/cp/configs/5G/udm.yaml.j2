logger:
    file: {{ default_core_log_dir_5g }}/udm.log

tls:
    enabled: {{ tls.enabled }}
    server:
      cacert: {{ default_core_configs_dir_5g }}/tls/ca.crt
      key: {{ default_core_configs_dir_5g }}/tls/udm.key
      cert: {{ default_core_configs_dir_5g }}/tls/udm.crt
    client:
      cacert: {{ default_core_configs_dir_5g }}/tls/ca.crt
      key: {{ default_core_configs_dir_5g }}/tls/udm.key
      cert: {{ default_core_configs_dir_5g }}/tls/udm.crt


udm:
    sbi:
      - addr: {{ udm.sbi }}
        port: 7777
